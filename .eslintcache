finiteScroll \n            listItems={VirtualScrollChildren} \n            lastRowHandler={lastRowHandler}\n        />\n    );\n}\n\nexport default VirtualAndInfiniteScroll; ","/Users/ipatrick/Codigos/ReactJS/demo-reactjs/src/components/virtualscroll/VirtualScroll.js",[],"/Users/ipatrick/Codigos/ReactJS/demo-reactjs/src/components/infinitescroll/InfiniteScroll.js",["123","124","125","126"],"\nimport React, {useEffect} from 'react'; \nimport { useInView } from 'react-intersection-observer';\nimport { DragDropContext, Droppable, Draggable } from \"react-beautiful-dnd\";\n\n/**\n * A container component for infinite scrolling. \n */\nfunction InfiniteScroll({listItems, lastRowHandler}) {\n    const [lastRowRef, lastRowInView] = useInView();\n\n    // Si es la última fila, llamamos al evento manejador\n    useEffect(() => {\n        lastRowInView && lastRowHandler();\n    }, [lastRowInView]);\n\n    const Elements = listItems.map((listItem,i) => {\n        const props = {key: i};\n        (i === listItems.length - 1) && (props['ref'] = lastRowRef);\n        return (\n            <div {...props}>\n                {listItem}\n            </div>\n        );\n    });\n    return (<>{Elements}</>);\n}\nexport default InfiniteScroll;","/Users/ipatrick/Codigos/ReactJS/demo-reactjs/src/redux/reducers/index.js",[],"/Users/ipatrick/Codigos/ReactJS/demo-reactjs/src/registerServiceWorker.js",[],{"ruleId":"127","severity":1,"message":"128","line":11,"column":13,"nodeType":"129","messageId":"130","endLine":11,"endColumn":26},{"ruleId":"127","severity":1,"message":"131","line":7,"column":27,"nodeType":"129","messageId":"130","endLine":7,"endColumn":32},{"ruleId":"132","severity":1,"message":"133","line":46,"column":29,"nodeType":"134","messageId":"135","endLine":46,"endColumn":31},{"ruleId":"127","severity":1,"message":"136","line":118,"column":21,"nodeType":"129","messageId":"130","endLine":118,"endColumn":30},{"ruleId":"132","severity":1,"message":"133","line":122,"column":21,"nodeT